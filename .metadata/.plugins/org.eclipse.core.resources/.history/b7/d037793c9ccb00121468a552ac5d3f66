package com.lewe.app.thread.safe;

public abstract class ThreadSafe extends Thread {
	
	private long TIMER_POLLING;
	
	private boolean stopThread;
	
	public ThreadSafe(long timerPolling) {
		
		super();
		
		TIMER_POLLING = timerPolling;
		
		stopThread = true;
	}
	
	public void start() {
		super
		();
	}

}
